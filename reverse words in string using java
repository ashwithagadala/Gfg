class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
        // code here 
        String res="";
        //S=S.trim();
        String s2[]=S.split("\\.");
        for(int i=s2.length-1;i>=0;i--){
            res+=s2[i];
            if(i!=0){
                res=res+".";
            }
        }
        return res;
    }
}
